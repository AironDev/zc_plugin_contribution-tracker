{
  "env": {
      "node": true,
      "commonjs": true,
      "es2021": true,
      "mocha": true
  },
  "extends": "eslint:recommended",
  "parser": "@babel/eslint-parser",
  "parserOptions": {
      "ecmaVersion": 12
  },
  "rules": {
      "indent": "off",
      "linebreak-style": 0,
      "no-irregular-whitespace":"off",
      "no-unused-vars": "off"
  }
}